<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Neighbor.Mobile.Views.User.RegisterContactPage">
    <ContentPage.Content>
        <Grid>
            <Label Text="User Contact"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand"
                FontSize="Title"/>

            <Entry AutomationId="RegisterContactHouseNumber"></Entry>
            <Entry AutomationId="RegisterContactPhoneNumber"></Entry>

            <Button Text="Submit"></Button>
        </Grid>
    </ContentPage.Content>
</ContentPage>